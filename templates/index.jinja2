{% extends "base.jinja2" %}
{% block body %}
   <ul>
	{% for entry in entries %}
	<li>
	   <a href="{{ request.route_url('detail', entryID=entry.id) }}">
	      {{ entry.title }}
	      <br>{{ entry.created.strftime('%b. %d, %Y') }}
	   </a>
	</li>
	{% endfor %}
   </ul>	
{% endblock %}
{% block nav %}
   <ul>
	 <li><a href="/">Home</a></li>
	 {% if request.authenticated_userid %}   
	 <li><a href="">new</a></li>
	 <li><a href="logout">log out</a></li>
	 {% else %}
	 <li><a href="login">log in</a></li>
   	 {% endif %}
   </ul>
{% endblock %}